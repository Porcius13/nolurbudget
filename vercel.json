{
    "version": 2,
    "framework": "vite",
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/app?path=$1"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}